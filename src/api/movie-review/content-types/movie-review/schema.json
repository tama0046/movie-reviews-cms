{
  "kind": "collectionType",
  "collectionName": "movie_reviews",
  "info": {
    "singularName": "movie-review",
    "pluralName": "movie-reviews",
    "displayName": "Movie Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "poster": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "review": {
      "type": "blocks",
      "required": true
    },
    "rating": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5
    },
    "year": {
      "type": "integer",
      "required": true,
      "min": 1900
    }
  }
}
